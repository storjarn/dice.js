{
  "name": "@postdawn/dice.js",
  "version": "1.0.0",
  "description": "random number generator and dice simulator",
  "main": "src/index.js",
  "bin": {
    "dice": "./bin/cli.js"
  },
  "scripts": {
    "build": "npm run clean:dist && npm run lint && tsc --inlineSources false --sourceMap false --removeComments false && gulp distBuild",
    "clean": "echo 'this will display an error on windows - feel free to ignore' && npm run clean:js && npm run clean:n || true",
    "clean:dist": "gulp cleanDist",
    "clean:js": "gulp cleanJS",
    "clean:n": "rimraf ./node_modules || true",
    "lint": "tslint --project ./",
    "test": "jest --config ./jest-config.json --useStderr --no-cache",
    "test:coverage": "jest --config ./jest-config.json --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storjarn/dice.js.git"
  },
  "keywords": [
    "random",
    "number",
    "generator",
    "dice",
    "node",
    "javascript",
    "typescript"
  ],
  "author": "Christopher Maples (cmaples77@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/storjarn/dice.js/issues"
  },
  "homepage": "https://github.com/storjarn/dice.js#readme",
  "dependencies": {
    "@types/del": "~4.0.0",
    "@types/gulp": "~4.0.6",
    "@types/mkdirp": "^1.0.1",
    "@types/rimraf": "^3.0.0",
    "colors": "^1.4.0",
    "concurrently": "~4.1.0",
    "cross-env": "~5.2.0",
    "del": "^4.1.0",
    "gulp": "~4.0.0",
    "gulp-chmod": "^3.0.0",
    "gulp-load-tasks": "^0.8.4",
    "gulp-rm-lines": "~0.0.9",
    "gulp-strip-comments": "~2.5.2",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.1",
    "tslint": "~6.1.2",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "~25.1.3",
    "jest": "~25.1.0",
    "ts-jest": "~25.2.1"
  }
}